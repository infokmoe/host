function showpageCount(e){for(var a,s=home_page_url,t=new Array,n=1,r=1,l=0,o=0,i=0,p="",g="",c="",h=0;a=e.feed.entry[h];h++){var u=a.published.$t.substring(0,19)+a.published.$t.substring(23,29);timestamp=encodeURIComponent(u);var d=a.title.$t;""!=d&&(0==l||l%pageCount==pageCount-1)&&(-1!=s.indexOf(timestamp)&&(n=r),""!=d&&r++,t[t.length]="/search?updated-max="+timestamp+"&max-results="+pageCount),l++}for(var m=0;m<t.length;m++)m>=n-displayPageNum-1&&m<n+displayPageNum&&(0==o&&m==n-2&&(g=2==n?'<span class="showpage"><a href="/">'+upPageWord+"</a></span>":'<span class="showpage"><a href="'+t[m]+'">'+upPageWord+"</a></span>",o++),p+=m==n-1?'<span class="showpagePoint">'+n+"</span>":0==m?'<span class="showpageNum"><a href="/">1</a></span>':'<span class="showpageNum"><a href="'+t[m]+'">'+(m+1)+"</a></span>",0==i&&m==n&&(c='<span class="showpage"><a href="'+t[m]+'">'+downPageWord+"</a></span>",i++));n>1&&(p=""+g+" "+p+" "),p='<div class="showpageArea"><span style="COLOR: #111111;" class="showpageOf">Pages ('+(r-1)+")</span>"+p,r-1>n&&(p+=c),1==r&&r++,p+="</div>";var f=document.getElementsByName("pageArea"),b=document.getElementById("blog-pager");2>=r&&(p="");for(var m=0;m<f.length;m++)f[m].innerHTML=p;f&&f.length>0&&(p=""),b&&(b.innerHTML=p)}function showpageCount2(e){var a=home_page_url,s=new Array,t=-1!=a.indexOf("/search/label/"),n=t?a.substr(a.indexOf("/search/label/")+14,a.length):"";n=-1!=n.indexOf("?")?n.substr(0,n.indexOf("?")):n;for(var r,l=1,o=1,i=0,p=0,g=0,c="",h="",u="",d='<span class="showpageNum"><a href="/search/label/'+n+"?&max-results="+pageCount+'">',a=home_page_url,m=0;r=e.feed.entry[m];m++){var f=r.published.$t.substring(0,19)+r.published.$t.substring(23,29);timestamp=encodeURIComponent(f);var b=r.title.$t;""!=b&&(0==i||i%pageCount==pageCount-1)&&(-1!=a.indexOf(timestamp)&&(l=o),""!=b&&o++,s[s.length]="/search/label/"+n+"?updated-max="+timestamp+"&max-results="+pageCount),i++}for(var w=0;w<s.length;w++)w>=l-displayPageNum-1&&w<l+displayPageNum&&(0==p&